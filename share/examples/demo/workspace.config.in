        "imports": {
            "std@zuri.dev": {
                "type": "Path",
                "path": "@ZURI_INSTALL_PACKAGES_DIR@/@ZURI_STD_PACKAGE_ZPK@"
            }
        },
        "targets": {
            "demo": {
                "type": "Program",
                "specifier": "@CHORD_DEMO_PACKAGE_NAME@-@CHORD_DEMO_PACKAGE_VERSION@@@CHORD_EXAMPLES_PACKAGE_GROUP@",
                "programMain": "/helloworld"
            }
        }
    }
}
